{
    "name": "@itboom/prisma-types",
    "version": "1.1.0",
    "description": "Global, type-safe utilities for working with Prisma Client operations and model types.",
    "author": "Bohdan Kulinchenko",
    "license": "MIT",
    "homepage": "https://github.com/ITBoomDev/prisma-types",
    "repository": {
        "type": "git",
        "url": "https://github.com/ITBoomDev/prisma-types.git"
    },
    "types": "./dist/index.d.ts",
    "exports": {
        "types": "./dist/index.d.ts"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "prepare": "mkdir -p dist && cp src/*.d.ts dist",
        "prepublishOnly": "bun run prepare",
        "prisma_generate": "prisma generate",
        "pub:patch": "npm version patch && bun publish --access public",
        "pub:minor": "npm version minor && bun publish --access public",
        "pub:major": "npm version major && bun publish --access public"
    },
    "devDependencies": {
        "@types/node": "^20.5.7",
        "typescript": "^5.1.6",
        "prisma": "^6.3.1"
    },
    "peerDependencies": {
        "@prisma/client": "^5.9.1"
    },
    "keywords": [
        "prisma",
        "prisma-types",
        "prisma client",
        "prisma client types",
        "prisma helper",
        "prisma utils",
        "prisma arguments",
        "prisma args",
        "prisma result",
        "prisma operations",
        "prisma model types",
        "prisma metadata",
        "typed prisma",
        "type-safe prisma",
        "orm",
        "orm types",
        "typescript",
        "typescript utils",
        "typescript prisma",
        "typed orm",
        "prisma query",
        "typed prisma query",
        "itboom"
    ]
}
